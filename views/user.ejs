<%- include header %>
<% posts.forEach(function (post, index) { %>
<p><h2><a href="/p/<%= post._id %>"><%= post.title %></a></h2></p>
<p class="info">
    作者：<a href="/u/<%= post.userId %>"><%= post.name %></a> |
    日期：<%= post.time.minute %>
</p>
<p><%- post.post %></p>
<div class="line"></div>
<% }) %>
<%- include paging %>
<%- include footer %>